{
    "name": "fidum/laravel-eloquent-morph-to-one",
    "description": "Adds MorphToOne relation to Laravel eloquent",
    "keywords": [
        "fidum",
        "laravel",
        "laravel-eloquent-morph-to-one"
    ],
    "homepage": "https://github.com/fidum/laravel-eloquent-morph-to-one",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Mason",
            "email": "fidum.dev@gmail.com",
            "homepage": "https://danielmason.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "illuminate/database": "^7.0",
        "illuminate/support": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0||^9.0",
        "psalm/plugin-laravel": "^1.2",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Fidum\\EloquentMorphToOne\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fidum\\EloquentMorphToOne\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
